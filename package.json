{
  "name": "workshop-frontend-minor",
  "version": "1.0.0",
  "private": true,
  "repository": "https://github.com/larsdouweschuitema/workshop-frontend-minor.git",
  "author": "Lars Douwe Schuitema <larsschuitema@funda.nl>",
  "license": "MIT",
  "scripts": {
    "server": "json-server server/houses.js",
    "client": "webpack-dev-server --open --config webpack.dev.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint --ext .js,.vue src",
    "ngrok": "ngrok http 8080 -host-header='localhost:8080'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint",
      "pre-push": "yarn run lint"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "7.8.4",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "5.1.0",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-vue": "6.2.1",
    "faker": "4.1.0",
    "file-loader": "5.0.2",
    "html-webpack-plugin": "5.5.0",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "jest-serializer-vue": "2.0.2",
    "json-server": "0.16.0",
    "mini-css-extract-plugin": "0.9.0",
    "ngrok": "3.2.7",
    "node-sass": "4.13.1",
    "sass-loader": "8.0.2",
    "vue-jest": "4.0.1",
    "vue-loader": "15.9.0",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "7.8.3",
    "axios": "0.19.2",
    "core-js": "2",
    "vue": "2.6.11",
    "vue-router": "3.1.5",
    "vuex": "3.1.2"
  },
  "engines": {
    "node": ">=10.0.0 <12.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
